{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "subscription",
    "description": "The subscription schema",
    "required": [
        "customer_number",
        "client_id"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the subscription."
        },
        "customer_number": {
            "$id": "#/properties/customer_number",
            "type": "string",
            "title": "The subscriptions customer_number"
        },
        "client_id": {
            "$id": "#/properties/client_id",
            "type": "integer",
            "title": "The subscriptions client_id"
        },
        "status_id": {
            "$id": "#/properties/status_id",
            "type": "integer",
            "title": "The status of the subscription."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the subscription."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the subscription was last updated."
        }
    }
}